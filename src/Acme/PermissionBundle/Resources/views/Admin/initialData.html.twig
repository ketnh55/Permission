{% extends "AcmePermissionBundle::Admin/baseAdmin.html.twig"%}
{%block title%}
<title>Khởi tạo dữ liệu cho hệ thống</title>
{%endblock%}
{%block pagetitle%}
     <h3>Khởi tạo dữ liệu cho hệ thống <small>Nguyễn Hữu Kết - K55C-CLC</small></h3>
{%endblock%}
{% block javascript%}
    {{parent()}}
    {% javascripts 
                    '@AcmePermissionBundle/Resources/public/bootstrap/js/plugins/forms/*'    
                       
    %}        
     <script type="text/javascript" src="{{ asset_url }}"></script>        
    {% endjavascripts %}     
{%endblock%}
{%block content %}
<div class="tabbable page-tabs">
      <ul class="nav nav-tabs">
        <li class="active"><a href="#inside" data-toggle="tab"><i class="icon-checkbox-partial"></i> Tạo mới người dùng</a></li>
        <li class=""><a href="#outside" data-toggle="tab"><i class="icon-checkbox-unchecked"></i> Tạo mới lĩnh vực hành chính</a></li>
        
        <li class=""><a href="#ok" data-toggle="tab"><i class="icon-checkmark3"></i>Tạo đơn vị thụ lý</a></li>
        <li class=""><a href="#advanced" data-toggle="tab"><i class="icon-checkmark3"></i> Tạo mới thủ tục hành chính</a></li>
      </ul>
      <div class="tab-content">
        <!-- First tab content -->
        <div class="tab-pane fade active in" id="inside">
            {{ render(controller('AcmePermissionBundle:Permission:createUser'))}}
        </div>
        <div class="tab-pane fade" id="outside">
            {{ render(controller('AcmePermissionBundle:Permission:createLinhvuc'))}}
        </div>
        <div class="tab-pane fade" id="advanced">
            {{ render(controller('AcmePermissionBundle:Permission:createTTHC'))}}
         </div>
         <div class="tab-pane fade" id="ok">
            {{ render(controller('AcmePermissionBundle:Permission:createDVTL'))}}
         </div>
      </div>
    </div>
{% endblock%}
